<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="509.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.cita350finalproject.IceCreamOrderController">
   <children>
      <Button fx:id="buttonCreateSandwichOrder" layoutX="340.0" layoutY="14.0" mnemonicParsing="false" onAction="#onCreateSandwichOrderClick" prefHeight="44.0" prefWidth="114.0" text="Create Sandwich Order" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Comic Sans MS Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="buttonReturnToMainScreen" layoutX="471.0" layoutY="14.0" mnemonicParsing="false" onAction="#onReturnToMainScreenClick" prefHeight="44.0" prefWidth="114.0" text="Return to Main Screen" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Comic Sans MS Italic" size="12.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="27.0" layoutY="14.0" prefHeight="44.0" prefWidth="281.0" text="Ice Cream Order" textAlignment="CENTER">
         <font>
            <Font name="Comic Sans MS Bold" size="30.0" />
         </font>
      </Label>
      <AnchorPane layoutX="14.0" layoutY="88.0" prefHeight="405.0" prefWidth="342.0">
         <children>
            <Button fx:id="buttonSubmit" layoutX="259.0" layoutY="365.0" mnemonicParsing="false" onAction="#onSubmitClick" text="Submit">
               <font>
                  <Font name="Comic Sans MS Italic" size="12.0" />
               </font></Button>
            <Label alignment="CENTER" layoutX="1.0" layoutY="29.0" prefHeight="17.0" prefWidth="60.0" text="Name" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Comic Sans MS Bold" size="12.0" />
               </font></Label>
            <Label alignment="CENTER" layoutX="1.0" layoutY="64.0" prefHeight="17.0" prefWidth="60.0" text="Quantity" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Comic Sans MS Bold" size="12.0" />
               </font></Label>
            <Label alignment="CENTER" layoutX="1.0" layoutY="96.0" text="\% Discount" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="Comic Sans MS Bold" size="12.0" />
               </font></Label>
            <Label layoutX="148.0" layoutY="134.0" text="Flavor">
               <font>
                  <Font name="Comic Sans MS Bold" size="12.0" />
               </font></Label>
            <Label layoutX="139.0" layoutY="241.0" text="Toppings">
               <font>
                  <Font name="Comic Sans MS Bold" size="12.0" />
               </font></Label>
            <TextField fx:id="textFieldQuantity" layoutX="70.0" layoutY="60.0" prefHeight="25.0" prefWidth="247.0">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></TextField>
            <TextField fx:id="textFieldDiscount" layoutX="70.0" layoutY="92.0" prefHeight="25.0" prefWidth="247.0">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></TextField>
            <TextField fx:id="textFieldName" layoutX="70.0" layoutY="25.0" prefHeight="25.0" prefWidth="247.0">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></TextField>
            <RadioButton fx:id="radioButtonChocolate" layoutX="44.0" layoutY="162.0" mnemonicParsing="false" text="Chocolate">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
               <toggleGroup>
                  <ToggleGroup fx:id="flavorGroup" />
               </toggleGroup></RadioButton>
            <RadioButton fx:id="radioButtonStrawberry" layoutX="44.0" layoutY="186.0" mnemonicParsing="false" text="Strawberry" toggleGroup="$flavorGroup">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></RadioButton>
            <RadioButton fx:id="radioButtonVanilla" layoutX="44.0" layoutY="213.0" mnemonicParsing="false" text="Vanilla" toggleGroup="$flavorGroup">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></RadioButton>
            <RadioButton fx:id="radioButtonRaspberry" layoutX="195.0" layoutY="160.0" mnemonicParsing="false" text="Raspberry" toggleGroup="$flavorGroup">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></RadioButton>
            <RadioButton fx:id="radioButtonCottonCandy" layoutX="194.0" layoutY="184.0" mnemonicParsing="false" text="Cotton Candy" toggleGroup="$flavorGroup">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></RadioButton>
            <RadioButton fx:id="radioButtonMint" layoutX="194.0" layoutY="211.0" mnemonicParsing="false" text="Mint" toggleGroup="$flavorGroup">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font></RadioButton>
            <CheckBox fx:id="checkBoxChocolateSprinkles" layoutX="45.0" layoutY="267.0" mnemonicParsing="false" text="Chocolate Sprinkles">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="checkBoxRainbowSprinkles" layoutX="44.0" layoutY="291.0" mnemonicParsing="false" text="Rainbow Sprinkles">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="checkBoxChocolateFudge" layoutX="44.0" layoutY="314.0" mnemonicParsing="false" text="Chocolate Fudge">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="checkBoxBrownieBits" layoutX="193.0" layoutY="267.0" mnemonicParsing="false" text="Brownie Bits">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="checkBoxMeltedMarshmallows" layoutX="193.0" layoutY="291.0" mnemonicParsing="false" text="Melted Marshmallows">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="checkBoxCookieDough" layoutX="193.0" layoutY="314.0" mnemonicParsing="false" text="Cookie Dough">
               <font>
                  <Font name="Comic Sans MS" size="12.0" />
               </font>
            </CheckBox>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="371.0" layoutY="88.0" prefHeight="405.0" prefWidth="200.0">
         <children>
            <Label layoutX="75.0" layoutY="14.0" text="Receipt">
               <font>
                  <Font name="Comic Sans MS Bold" size="14.0" />
               </font></Label>
            <Label fx:id="labelReceipt" alignment="TOP_LEFT" layoutX="14.0" layoutY="58.0" prefHeight="338.0" prefWidth="176.0" wrapText="true">
               <font>
                  <Font name="Comic Sans MS" size="16.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
